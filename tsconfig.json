  "compilerOptions": {
    "jsx": "react",
    "sourceMap": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
    "exclude": [
        "node_modules"
    ]
}